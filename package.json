{
    "name": "SeeleMist",
    "version": "1.0.0",
    "icon": "seeleMist.icns",
    "description": "A minimal Electron application",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "packager": "electron-packager . seeleMist --all --out=../dist/ --overwrite=true --ignore=node_modules/electron-* --ignore=node_modules/.bin --ignore=.git --ignore=dist --icon=seeleMist.ico",
        "dist": "electron-builder"
    },
    "build": {
        "appId": "seeleMist",
        "dmg": {
            "title": "SeeleMist",
            "icon": "seeleMist.icns"
        },
        "mac": {
            "asar": true,
            "target": "dmg",
            "icon": "seeleMist.icns"
        },
        "extraResources": {
            "from": "./cmd/",
            "to": "../"
        }
    },
    "repository": "https://github.com/seeleteam/SeeleMist",
    "keywords": [
        "Electron",
        "quick",
        "start",
        "tutorial",
        "demo"
    ],
    "author": "Seeleteam",
    "license": "CC0-1.0",
    "devDependencies": {
        "electron": "^3.0.4"
    },
    "dependencies": {
        "bluebird": "^3.5.2",
        "seele.js": "^1.5.1"
    }
}